{
    "name": "orkhanahmadov/laravel-ip-middleware",
    "description": "Laravel middleware for whitelisting/blacklisting client IP addresses",
    "keywords": [
        "orkhanahmadov",
        "laravel",
        "middleware",
        "ip-filter",
        "ip-whitelist",
        "ip-blacklist",
        "laravel-ip-middleware"
    ],
    "homepage": "https://github.com/orkhanahmadov/laravel-ip-middleware",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\LaravelIpMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\LaravelIpMiddleware\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orkhanahmadov\\LaravelIpMiddleware\\LaravelIpMiddlewareServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
